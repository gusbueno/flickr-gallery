{
  "name": "flickr-gallery",
  "version": "1.0.0",
  "description": "Images gallery from Flickr API",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack-dev-server --mode development",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gusbueno/flickr-gallery.git"
  },
  "keywords": [
    "flickr",
    "react",
    "redux",
    "webpack"
  ],
  "author": "Gustavo Bueno",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gusbueno/flickr-gallery/issues"
  },
  "homepage": "https://github.com/gusbueno/flickr-gallery#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.3",
    "react-test-renderer": "^16.3.2",
    "sass-loader": "^7.0.1",
    "standard": "^11.0.1",
    "style-loader": "^0.20.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "babel": {
    "presets": [
      "babel-preset-env",
      "babel-preset-react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/__tests__/setupTest.js",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/js/**"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "__tests__/setupTest.js"
    ]
  }
}
